<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.hotelbooking">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.HotelBooking">
        <activity
            android:name="com.example.hotelbooking.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.hotelbooking.RegisterActivity"
            android:exported="true">
        </activity>
        <activity
            android:name="com.example.hotelbooking.UserDetailsActivity"
            android:exported="true">
        </activity>
        <activity
            android:name="com.example.hotelbooking.RegisteredUsersActivity"
            android:exported="true">
        </activity>
    </application>

</manifest>